            game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(unpack(args))
        end
    end
end)
    
folder4:AddSwitch("Auto Buy Skills", function(state)  
    _G.sk = state
    if _G.sk then
        while _G.sk do
            wait()
            local args = {
                [1] = "buyAllSkills",
                [2] = "Blazing Vortex Island"
            }
            game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(unpack(args))
        end
    end
end)
    
folder4:AddSwitch("Auto Buy Ranks", function(state) 
    _G.r = state
    if _G.r then
        while _G.r do
            wait()
            local oh1 = "buyRank"
            local oh2 = game:GetService("ReplicatedStorage").Ranks.Ground:GetChildren()
            for i = 1, #oh2 do
                game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(oh1, oh2[i].Name)
            end
        end
    end
end)
    
folder4:AddSwitch("Auto Buy Shurikens", function(state)
    _G.sh = state
    if _G.sh then
        while _G.sh do
            wait()
            local args = {
                [1] = "buyAllShurikens",
                [2] = "Blazing Vortex Island"
            }
            game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(unpack(args))
        end
    end
end)
    
    
local Misc = window:AddTab("Misc") 
local folder5 = Misc:AddFolder("Settings V2")  
    
folder5:AddButton("Disable Trading", function()
 local args = { [1] = "disableTrading" }
    game:GetService("ReplicatedStorage").rEvents.tradingEvent:FireServer(unpack(args))
end)
    
folder5:AddButton("Enable Trading", function()
local args = { [1] = "enableTrading" }
    game:GetService("ReplicatedStorage").rEvents.tradingEvent:FireServer(unpack(args))
end)
    
folder5:AddButton("Auto Spin Wheel", function()
    autospinwheel = true
    if autospinwheel then
        game:GetService("ReplicatedStorage").rEvents.openFortuneWheelRemote:InvokeServer("openFortuneWheel", game:GetService("ReplicatedStorage").fortuneWheelChances["Fortune Wheel"])
        task.wait(1)
    end
end)
    
local ServerStats = window:AddTab("Server Stats")
local Time = Main:Label("Executer Time")

local function UpdateTime()
    local GameTime = math.floor(workspace.DistributedGameTime + 0.5)
    local Hour = math.floor(GameTime / (60^2)) % 24
    local Minute = math.floor(GameTime / 60) % 60
    local Second = math.floor(GameTime) % 60
    Time:Set("[GameTime] : Hours : " .. Hour .. " Minutes : " .. Minute .. " Seconds : " .. Second)
end

spawn(function()
    while true do
        wait()
        pcall(function()
            UpdateTime()
        end)
    end
end)

local Client = Main:Label("Client")

local function UpdateClient()
    local Fps = workspace:GetRealPhysicsFPS()
    Client:Set("[Fps] : " .. Fps)
end

spawn(function()
    while true do
        wait(0.1)
        UpdateClient()
    end
end)

local Client1 = Main:Label("Client")

local function UpdateClient1()
    local Ping = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()
    Client1:Set("[Ping] : " .. Ping)
end

spawn(function()
    while true do
        wait(0.1)
        UpdateClient1()
    end
end)
